{
  "name": "@velotype/velotype",
  "license": "MIT",
  "version": "0.0.1",
  "exports": {
    ".": "./src/tsx/tsx-core.ts",
    "./tsx": "./src/tsx/tsx-core.ts"
  },
  "publish": {
    "exclude": [
      "docs/",
      "tests/"
    ]
  },
  "tasks": {
    "bundle-tsx-core": "deno bundle --minify ./src/tsx/tsx-core.ts --output ./dist/tsx-core.min.js --sourcemap",
    "test": "deno test ./tests/"
  },
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "target": "esnext"
  },
  "lint": {
    "rules": {
      "exclude": ["jsx-key"]
    }
  }
}